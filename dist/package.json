{
  "name": "zoomist",
  "version": "2.2.0",
  "description": "A TypeScript library for zooming any element. Also supports mobile devices.",
  "type": "module",
  "main": "./zoomist.umd.js",
  "module": "./zoomist.js",
  "style": "./zoomist.css",
  "types": "./zoomist.d.ts",
  "exports": {
    ".": {
      "types": "./zoomist.d.ts",
      "default": "./zoomist.js"
    },
    "./css": "./zoomist.css",
    "./types": "./types.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cotton123236/zoomist.git"
  },
  "bugs": {

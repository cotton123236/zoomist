    "zoom",
    "zoomist",
    "zoomer",
    "zooming",
    "pinch",
    "pinchzoom",
    "gesture",
    "touch",
    "touchzoom",
    "touchmove",
    "mobile",
    "javascript",
    "front-end"
  ],
  "author": "Wilson Wu",
